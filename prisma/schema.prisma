generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id            Int         @id @default(autoincrement())
  login         String      @unique
  senha         String
  user_nivel    User_Nivel? @relation(fields: [user_nivel_id], references: [id])
  user_nivel_id Int?
  Pessoa        Pessoa[]

  @@map("usuarios")
}

model User_Nivel {
  id        Int    @id @default(autoincrement())
  descricao String @unique
  users     User[]

  @@map("user_nivel")
}

model Pessoa {
  id              Int           @id @default(autoincrement())
  nome            String        @unique
  identidade      String
  cpf             String        @unique
  dt_nasc         DateTime
  tel             String
  email           String
  user            User?         @relation(fields: [user_id], references: [id])
  user_id         Int?
  tipo_desc       Pessoa_tipo?  @relation(fields: [tipo_id], references: [id])
  tipo_id         Int?
  sexo_desc       Sexo?         @relation(fields: [sexo_id], references: [id])
  sexo_id         Int?
  enderecos       Enderecos?    @relation(fields: [enderecos_id], references: [id])
  enderecos_id    Int?
  ativo_status    Ativo_status? @relation(fields: [ativo_status_id], references: [id])
  ativo_status_id Int?

  created_at DateTime  @default(now())
  updated_at DateTime  @updatedAt
  deleted_at DateTime?

  @@map("pessoas")
}

model Pessoa_tipo {
  id        Int      @id @default(autoincrement())
  descricao String   @unique
  pessoas   Pessoa[]

  @@map("pessoa_tipo")
}

model Ativo_status {
  id        Int       @id @default(autoincrement())
  descricao String    @unique
  pessoas   Pessoa[]
  produtos  Produto[]

  @@map("pessoa_status")
}

model Sexo {
  id        Int      @id @default(autoincrement())
  descricao String
  pessoas   Pessoa[]

  @@map("sexo")
}

model Enderecos {
  id          Int      @id @default(autoincrement())
  endereco    String
  bairro      String
  complemento String
  ponto_ref   String
  cep         String
  uf          String
  pessoas     Pessoa[]

  created_at DateTime  @default(now())
  updated_at DateTime  @updatedAt
  deleted_at DateTime?

  @@map("enderecos")
}

model Produto {
  id                Int              @id @default(autoincrement())
  descricao         String           @unique
  cod_barras        String
  vl_custo          Decimal
  vl_venda          Decimal
  vl_aluguel        Decimal
  qtd_estoque       Int
  prod_imagem       String
  ativo_status      Ativo_status?    @relation(fields: [ativo_status_id], references: [id])
  ativo_status_id   Int?
  prod_tamanho      Prod_tamanho?    @relation(fields: [prod_tamanho_id], references: [id])
  prod_tamanho_id   Int?
  prod_compri       Prod_compri?     @relation(fields: [prod_compri_id], references: [id])
  prod_compri_id    Int?
  prod_cor          Prod_cor?        @relation(fields: [prod_cor_id], references: [id])
  prod_cor_id       Int?
  prod_fabrica      Prod_fabrica?    @relation(fields: [prod_fabrica_id], references: [id])
  prod_fabrica_id   Int?
  prod_categoria    Prod_categoria?  @relation(fields: [prod_categoria_id], references: [id])
  prod_categoria_id Int?
  estoque_lanc      Estoque_lanc?    @relation(fields: [estoque_lanc_id], references: [id])
  estoque_lanc_id   Int?
  contrato_itens    Contrato_itens[]

  created_at DateTime  @default(now())
  updated_at DateTime  @updatedAt
  deleted_at DateTime?

  @@map("produtos")
}

model Prod_tamanho {
  id        Int       @id @default(autoincrement())
  descricao String
  produtos  Produto[]

  @@map("prod_tamanho")
}

model Prod_compri {
  id        Int       @id @default(autoincrement())
  descricao String
  produtos  Produto[]

  @@map("prod_compri")
}

model Prod_cor {
  id        Int       @id @default(autoincrement())
  descricao String
  produtos  Produto[]

  @@map("prod_cor")
}

model Prod_fabrica {
  id        Int       @id @default(autoincrement())
  descricao String
  produtos  Produto[]

  @@map("prod_fabrica")
}

model Prod_categoria {
  id        Int       @id @default(autoincrement())
  descricao String
  produtos  Produto[]

  @@map("prod_categoria")
}

model Estoque_lanc {
  id              Int           @id @default(autoincrement())
  descricao       String
  qtd             Int
  data            DateTime
  produtos        Produto[]
  estoque_tipo    Estoque_tipo? @relation(fields: [estoque_tipo_id], references: [id])
  estoque_tipo_id Int?
  estoque_oper    Estoque_oper? @relation(fields: [estoque_oper_id], references: [id])
  estoque_oper_id Int?

  created_at DateTime  @default(now())
  updated_at DateTime  @updatedAt
  deleted_at DateTime?

  @@map("estoque_lanc")
}

model Estoque_tipo {
  id           Int            @id @default(autoincrement())
  descricao    String
  estoque_lanc Estoque_lanc[]

  @@map("estoque_tipo")
}

model Estoque_oper {
  id           Int            @id @default(autoincrement())
  descricao    String
  estoque_lanc Estoque_lanc[]

  @@map("estoque_oper")
}

model Contrato {
  id                 Int              @id @default(autoincrement())
  vl_total           Decimal
  dt_prova           DateTime
  dt_saida           DateTime
  dt_devol           DateTime
  contrato_status    Contrato_status? @relation(fields: [contrato_status_id], references: [id])
  contrato_status_id Int?
  contrato_itens     Contrato_itens?  @relation(fields: [contrato_itens_id], references: [id])
  contrato_itens_id  Int?

  created_at DateTime     @default(now())
  updated_at DateTime     @updatedAt
  deleted_at DateTime?
  caixa_lanc Caixa_lanc[]

  @@map("contratos")
}

model Contrato_itens {
  id         Int        @id @default(autoincrement())
  qtd        Int
  valor      Decimal
  contratos  Contrato[]
  produto    Produto?   @relation(fields: [produto_id], references: [id])
  produto_id Int?

  created_at DateTime  @default(now())
  updated_at DateTime  @updatedAt
  deleted_at DateTime?

  @@map("contrato_itens")
}

model Contrato_status {
  id        Int        @id @default(autoincrement())
  descricao String
  contratos Contrato[]

  @@map("contrato_status")
}

model Caixa_lanc {
  id        Int      @id @default(autoincrement())
  descricao String
  valor     Decimal
  data      DateTime

  caixa_form_pag    Caixa_form_pag? @relation(fields: [caixa_form_pag_id], references: [id])
  caixa_form_pag_id Int?
  caixa_cate        Caixa_cate?     @relation(fields: [caixa_cate_id], references: [id])
  caixa_cate_id     Int?
  contrato          Contrato?       @relation(fields: [contrato_id], references: [id])
  contrato_id       Int?
  venda             Venda?          @relation(fields: [vendasId], references: [id])
  vendasId          Int?

  @@map("caixa_lanc")
}

model Caixa_form_pag {
  id        Int          @id @default(autoincrement())
  descricao String
  caixa     Caixa_lanc[]

  @@map("caixa_form_pag")
}

model Caixa_cate {
  id        Int          @id @default(autoincrement())
  descricao String
  caixa     Caixa_lanc[]

  @@map("caixa_cate")
}

model Venda {
  id            Int           @id @default(autoincrement())
  vl_total      Decimal?
  desconto_val  Decimal?
  desconto_per  Decimal?
  vl_descontado Decimal?
  finalizada    Boolean       @default(false)
  venda_itens   Venda_itens[]
  caixa_lanc    Caixa_lanc[]

  created_at DateTime  @default(now())
  updated_at DateTime  @updatedAt
  deleted_at DateTime?

  @@map("vendas")
}

model Venda_itens {
  id        Int     @id @default(autoincrement())
  qtd       Int
  vl_uni    Decimal
  vl_total  Decimal
  venda     Venda?  @relation(fields: [vendas_id], references: [id])
  vendas_id Int?

  @@map("venda_itens")
}

model Evento {
  id             Int          @id @default(autoincrement())
  data           DateTime
  descricao      String
  created_at     DateTime     @default(now())
  updated_at     DateTime     @updatedAt
  deleted_at     DateTime?
  evento_tipo    Evento_tipo? @relation(fields: [evento_tipo_id], references: [id])
  evento_tipo_id Int?

  @@map("eventos")
}

model Evento_tipo {
  id        Int      @id @default(autoincrement())
  descricao String
  eventos   Evento[]

  @@map("evento_tipo")
}
